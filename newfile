pipeline{
    agent any 
    stages{
      stage('1-step one'){
        steps{
          echo " I am step one"
        }
      }
      stage('2-second stage'){
        steps{
          echo "I am the second step"
        }
      }
      stage('3-this is step 3'){
        steps{
          echo " walking the third time"
        }
      }
      stage('4-The fourth move'){
        steps{
          echo "This is the forth step"
        }
      }
    }
  }
